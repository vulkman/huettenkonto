#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/cli.js/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules:/Users/volkerbach/Documents/workspaces/bachv/node_modules:/Users/volkerbach/Documents/workspaces/node_modules:/Users/volkerbach/Documents/node_modules:/Users/volkerbach/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../html-minifier/3.5.21/node_modules/html-minifier/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/cli.js/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/html-minifier/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules/.pnpm/node_modules:/Users/volkerbach/Documents/workspaces/bachv/huettenkonto/node_modules:/Users/volkerbach/Documents/workspaces/bachv/node_modules:/Users/volkerbach/Documents/workspaces/node_modules:/Users/volkerbach/Documents/node_modules:/Users/volkerbach/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../html-minifier/3.5.21/node_modules/html-minifier/cli.js" "$@"
